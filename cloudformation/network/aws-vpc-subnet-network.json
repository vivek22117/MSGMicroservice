{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Cloudformatin template for VPC and subnets",
  "Parameters": {
    "VpcCidrBlock": {
      "Description": "Vpc CIDR block range",
      "Type": "String",
      "Default": "10.0.0.0/22"
    },
    "Environment" : {
      "Description" : "Environment to which cft is created",
      "Type" : "String",
      "Default" : "devl"
    }
  },
  "Resources": {
    "DoubleDigitVPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": {
          "Ref": "VpcCidrBlock"
        },
        "EnableDnsSupport": "true",
        "EnableDnsHostnames": "true",
        "InstanceTenancy": "default",
        "Tags": [
          {
            "Key": "Name",
            "Value": "double-digit-vpc"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          }
        ]
      }
    },
    "PublicSubnetA": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "DoubleDigitVPC"
        },
        "CidrBlock": "10.0.0.0/24",
        "AvailabilityZone": "us-east-1a",
        "MapPublicIpOnLaunch": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": "dd-public-subnet-1a"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          },
          {
            "Key": "Scope",
            "Value": "public"
          }
        ]
      }
    },
    "PublicSubnetB": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "DoubleDigitVPC"
        },
        "CidrBlock": "10.0.1.0/24",
        "AvailabilityZone": "us-east-1b",
        "MapPublicIpOnLaunch": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": "dd-public-subnet-1b"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          },
          {
            "Key": "Scope",
            "Value": "public"
          }
        ]
      }
    },
    "PrivateSubnetA": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "DoubleDigitVPC"
        },
        "CidrBlock": "10.0.2.0/25",
        "AvailabilityZone": "us-east-1a",
        "MapPublicIpOnLaunch": false,
        "Tags": [
          {
            "Key": "Name",
            "Value": "dd-private-subnet-1a"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          },
          {
            "Key": "Scope",
            "Value": "private"
          }
        ]
      }
    },
    "PrivateSubnetB": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "DoubleDigitVPC"
        },
        "CidrBlock": "10.0.2.128/25",
        "AvailabilityZone": "us-east-1b",
        "MapPublicIpOnLaunch": false,
        "Tags": [
          {
            "Key": "Name",
            "Value": "dd-private-subnet-1b"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          },
          {
            "Key": "Scope",
            "Value": "private"
          }
        ]
      }
    },
    "PrivateSubnetDBOne": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "DoubleDigitVPC"
        },
        "CidrBlock": "10.0.3.0/25",
        "AvailabilityZone": "us-east-1b",
        "MapPublicIpOnLaunch": false,
        "Tags": [
          {
            "Key": "Name",
            "Value": "DB-dd-private-subnet-1b"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          },
          {
            "Key": "Scope",
            "Value": "private"
          }
        ]
      }
    },
    "PrivateSubnetDBTwo": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "DoubleDigitVPC"
        },
        "CidrBlock": "10.0.3.128/25",
        "AvailabilityZone": "us-east-1c",
        "MapPublicIpOnLaunch": false,
        "Tags": [
          {
            "Key": "Name",
            "Value": "DB-dd-private-subnet-1c"
          },
          {
            "Key": "component",
            "Value": "double-digit-test"
          },
          {
            "Key": "environment",
            "Value": {"Ref": "Environment"}
          },
          {
            "Key": "Scope",
            "Value": "private"
          }
        ]
      }
    }
  },
  "Outputs": {
    "StackVPC": {
      "Description": "The ID of the VPC",
      "Value": {
        "Ref": "DoubleDigitVPC"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-VPCId"
        }
      }
    },
    "VPCCidr": {
      "Description": "The CIDR Block of the VPC",
      "Value": {
          "Fn::GetAtt": [
            "DoubleDigitVPC",
            "CidrBlock"
          ]
        },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-VPCCidrBlock"
        }
      }
    },
    "PublicSubnetAId": {
      "Description": "The ID of Public Subnet A",
      "Value": {
        "Ref": "PublicSubnetA"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-SubnetId-A"
        }
      }
    },
    "PublicSubnetBId": {
      "Description": "The ID of Public Subnet B",
      "Value": {
        "Ref": "PublicSubnetB"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-SubnetId-B"
        }
      }
    },
    "PrivateSubnetBId": {
      "Description": "The ID of Private Subnet B",
      "Value": {
        "Ref": "PrivateSubnetB"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-PrivateSubnetId-B"
        }
      }
    },
    "PrivateSubnetAId": {
      "Description": "The ID of Private Subnet A",
      "Value": {
        "Ref": "PrivateSubnetA"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-PrivateSubnetId-A"
        }
      }
    },
    "PrivateSubnetDBIdA": {
      "Description": "The ID of DB Private Subnet",
      "Value": {
        "Ref": "PrivateSubnetDBOne"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-PrivateSubnetIdA-DB"
        }
      }
    },
    "PrivateSubnetDBIdB": {
      "Description": "The ID of DB Private Subnet",
      "Value": {
        "Ref": "PrivateSubnetDBTwo"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-PrivateSubnetIdB-DB"
        }
      }
    }
  }
}